{
  "name": "pr1-sass-less",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build:sass": "sass styles/main.scss public/main.css",
    "watch:sass": "sass --watch styles/main.scss:public/main.css",
    "build:less": "lessc styles/main.less public/main-less.css",
    "watch:less": "lessc --watch styles/main.less public/main-less.css"
  },
  "devDependencies": {
    "sass": "^1.70.0",
    "less": "^4.2.0"
  }
}